{
  "name": "nextjs-13beta-ecommerce",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": ["npm format", "npm lint:eslint --fix"]
  },
  "browser": {
    "fs": false,
    "net": false,
    "dns": false,
    "tls": false,
    "assert": false,
    "child_process": false
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write **/*.{ts,tsx}",
    "lint:eslint": "eslint . --ext .ts,.tsx",
    "lint:stylelint": "stylelint \"**/*.scss\" \"**/*.css\" --fix",
    "prisma:run:script": "ts-node --esm ./prisma/script.mts",
    "prisma:seed": "prisma db seed",
    "prisma:migrate": "prisma migrate dev --name init",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next/font": "13.1.2",
    "@paypal/react-paypal-js": "^7.8.3",
    "@prisma/client": "^4.13.0",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "classnames": "^2.3.2",
    "csv-parser": "^3.0.0",
    "formidable": "1.2.2",
    "next": "^13.1.6",
    "next-auth": "^4.22.1",
    "next-redux-wrapper": "^8.1.0",
    "nodemailer": "^6.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "sass": "^1.63.6",
    "ts-node": "^10.9.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/bcrypt": "^5.0.0",
    "@types/formidable": "^2.0.6",
    "@types/node": "18.11.18",
    "@types/nodemailer": "^6.4.8",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.4.9",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prisma": "^4.13.0",
    "stylelint": "^15.3.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-prettier": "^3.0.0",
    "tailwindcss": "^3.2.4"
  }
}
